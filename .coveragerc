[run]
source =
    mimeparser
    xml2dict

omit =
    *tests*
    *defusedxml*
    *six*
    *decimal.py
    *numbers.py
    *xml/etree*

[report]
show_missing = True
skip_covered = True
precision = 3

# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

[html]
title = Inventory Coverage Report
directory = docs/htmlcov
